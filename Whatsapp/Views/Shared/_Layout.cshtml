                var Tinycon = {};
                var currentFavicon = null;
                var originalFavicon = null;
                var faviconImage = null;
                var canvas = null;
                var options = {};
                // Chrome browsers with nonstandard zoom report fractional devicePixelRatio.
                var r = Math.ceil(window.devicePixelRatio) || 1;
                var size = 16 * r;
